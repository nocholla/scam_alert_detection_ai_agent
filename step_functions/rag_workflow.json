{
  "Comment": "RAG pipeline for Scam Alert Detection",
  "StartAt": "PreprocessProfile",
  "States": {
    "PreprocessProfile": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:123456789012:function:scam-alert-preprocess",
      "Next": "RunPredictions"
    },
    "RunPredictions": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:123456789012:function:scam-alert-predict",
      "Next": "RunRAGPipeline"
    },
    "RunRAGPipeline": {
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:123456789012:function:scam-alert-rag",
      "End": true
    }
  }
}
